  "scripts": {
    "db:up": "yarn db:migrate:up && yarn db:seed",
    "db:migrate:up": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "queue:makeready": "node --import jiti/register scripts/run-job.ts makeready-parse-list-page",
    "queue:atlima": "node --import jiti/register scripts/run-job.ts atlima-parse-list-page",
    "queue:matchday": "node --import jiti/register scripts/run-job.ts matchday-parse-list-page",
    "queue:pending": "node --import jiti/register scripts/run-job.ts perform-links-pending"
    
  },
  "dependencies": {
    "@types/sanitize-html": "^2.13.0",
    "cheerio": "^1.0.0",
    "jiti": "^2.4.2",
    "sanitize-html": "^2.14.0",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "sqlite3": "^5.1.7"
  }
}
